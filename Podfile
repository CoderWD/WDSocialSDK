# Uncomment this line to define a global platform for your project



target "WDSocialSDK" do
    platform :ios, '8.0'
    source 'https://github.com/CocoaPods/Specs.git'
    use_frameworks!
    pod 'WechatOpenSDK', '~> 1.7.8'
    pod 'WDTencentSDK'
    pod 'WDWeiboSDK'

end

target "WDSocialSDKExample" do
    platform :ios, '8.0'
    source 'https://github.com/CocoaPods/Specs.git'
    use_frameworks!
    pod 'WDSocialSDK'

end
